{
  "name": "@kronos-integration/service-repositories",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-repositories.mjs",
  "exports": {
    ".": "./src/service-repositories.mjs"
  },
  "description": "repository provider",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-repositories.mjs --section=API",
    "lint": "documentation lint ./src/service-repositories.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^9.2.2",
    "aggregation-repository-provider": "^2.8.8"
  },
  "devDependencies": {
    "ava": "^3.11.1",
    "c8": "^7.3.0",
    "documentation": "^13.0.2",
