    "github-repository-provider": "^7.15.4",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=14.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-repositories.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-repositories/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-repositories#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
