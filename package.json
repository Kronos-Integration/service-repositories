    "github-repository-provider": "^10.0.20",
    "semantic-release": "^25.0.3"
  },
  "engines": {
    "node": ">=22.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/service-repositories.git"
  },
  "bugs": {
